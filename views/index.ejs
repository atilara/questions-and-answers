<%- include('partials/header'); -%>
  <!-- forEach para renderizar cada question -->
  <% questions.forEach(question => { %>
    <div class="mt-4 card">
      <div class="card-body">
        <h3><%= question.title %></h3>
      </div>
      <div class="card-footer">
       <a href="/question/<%= question.id %>"><button class="btn btn-dark">Abrir</button></a>
      </div>
    </div>
  <% }); %>
<%- include('partials/footer'); -%>
